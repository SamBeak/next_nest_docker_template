{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "overrides": [
    {
    "files": "*.ts",
    "options": {
      "parser": "typescript"
    }
    }
  ]
}
